{
    "files.associations": {
        "poetry.lock": "toml",
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__": true,
    },
    "rust-client.channel": "nightly",
    "python.pythonPath": ".venv/bin/python",
    "python.formatting.provider": "yapf",
}